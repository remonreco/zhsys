* { padding: 0; margin: 0; }
html, body { height: 100%;font-family: 'Microsoft YaHei';}
li { list-style: none; }
.clear{-zoom:1;clear:both;overflow:hidden;}
.clear:after{content:"";display:block;height:0;clear:both;}
.clear_{clear:both;height:0;overflow:hidden}
div, a { -webkit-tap-highlight-color: rgba(255,0,0,0); text-decoration: none; outline:none }
img, fieldset, iframe{ border: 0; }
input, select, img { vertical-align: middle; border: none; outline: none; }
input{outline:none;}
em, i ,del ,ins{ font-style: normal; text-decoration: none;}
span, i, em ,b,strong { font-weight:normal;}

