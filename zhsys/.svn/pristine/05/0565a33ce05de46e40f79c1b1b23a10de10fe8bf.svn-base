			 and a.order_type=#{orderType}
		</if>
		<if test="orderState != null and orderState !=''">
			 and a.order_state=#{orderState}
		</if>
		<if test="orderTimeStart != null and orderTimeStart !=''">
 	 		 and a.order_time &gt;= #{orderTimeStart}
      	</if> 
      	<if test="orderTimeEnd != null and orderTimeEnd !=''">
 	 		 and a.order_time &lt;= #{orderTimeEnd}
      	</if> 
	  	
	  	order by user_id,exchange_id,order_time;

  </select>
  
  <select id="selectByUserId" parameterType="map" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from hs_exchange_order
    where user_id = #{userId} and sys_id = #{sysId}
    order by order_time DESC
      limit #{limit} offset #{offset} 
  </select>
  
  <select id="selectCountByUserId" parameterType="map" resultType="int">
    select count(1) from(select 
    <include refid="Base_Column_List" />
    from hs_exchange_order
    where user_id = #{userId} and sys_id = #{sysId}
    order by order_time DESC
      )aa 
  </select>
  
  <select id="queryExchangeOrderAndGift" parameterType="map" resultType="com.cbice.distribute.core.db.model.HsExchangeOrder">
	  select a.id,b.gift_id giftId,b.gift_name giftName,b.gift_num giftNum from hs_exchange_order a,hs_exchange_gift b 
	  	where  a.id = #{id} and a.id = b.order_id  and a.sys_id = #{sysId}
	  	<if test="orderType != null and orderType !=''">
			 and a.order_type=#{orderType}
		</if>
  </select>
  
  <select id="selectByPrimaryKeyAndSysId" parameterType="map" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from hs_exchange_order
    where id = #{id} and sys_id = #{sysId}
  </select>
</mapper>